- form_for @user, :url => admin_employees_path do |f|
  %p
    = f.label :login
  %p
    = f.text_field :login
  %p
    = f.label :email
  %p
    = f.text_field :email
  %p
    = f.label :password
  %p
    = f.text_field :password
  %p
    = f.label :password_confirmation
  %p
    = f.password_field :password_confirmation
  %p
    = f.submit "Register"